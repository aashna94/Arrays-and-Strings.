#include <iostream>
#include <vector>
#include <queue>
#include <stack>
#include <string>
#include <climits>
#include <stdio.h>
#include <string.h>
#include <cassert>
#include <algorithm>
#include <stack>
#include <map>
#include <list>
#include <set>
#include <math.h>

using namespace std;

void contiguousSum(int *a, int n)
{
    int i,maxi=0,sum=0;
    for (i=0; i<n ; i++)
    {
        sum+=a[i];
        if(maxi<sum)
            maxi=sum;
        if(sum<0)
            sum=0;
    }
    cout<<maxi;
}

int main(int argc, const char * argv[])
{
    int i,n;
    cin>>n;
    int *a=new int[n];
    for (i=0; i<n ; i++)
    {
        cin>>a[i];
    }
    contiguousSum(a, n);
    return 0;
}

