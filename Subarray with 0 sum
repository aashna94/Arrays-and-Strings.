#include <iostream>
#include <vector>
#include <queue>
#include <unordered_set>
#include <string>
#include <climits>
#include <cassert>
#include <map>
#include <set>

using namespace std;

void maxSubarray(int *ar, int n)
{
    map<int, int> m;
    int i,sum=0,len=0;
    for (i=0; i<n ; i++)
    {
        sum+=ar[i];
        if(ar[i]==0 && len==0)
            len=1;
        if(sum==0)
            len=i+1;
        if(m.find(sum)!=m.end())
            len=max(len, i-m[sum]);
        else
            m[sum]=i;
    }
    cout<<len;
}

int main(int argc, const char * argv[])
{
   
    int i,n,ele;
    cin>>n;
    int* ar=new int[n-2];
    for (i=0; i<n-2; i++)
    {
        cin>>ar[i];
    }
    maxSubarray(ar, n);
    return 0;
}
