#include <iostream>
#include <vector>
#include <queue>
#include <unordered_set>
#include <string>
#include <climits>
#include <cassert>
#include <set>

using namespace std;

void massuese(int *ar, int n)
{
    int x=0,y=0,i,j,xori,bit;
    xori=ar[0];
    for (i=1; i<n-2 ; i++)
    {
        xori^=ar[i];
    }
    for (i=1; i<=n ; i++)
    {
        xori^=i;
    }
    bit=xori ^ ~(xori-1);
    for (i=0; i<n-2; i++)
    {
        if(ar[i] & bit)
            x^=ar[i];
        else
            y^=ar[i];
    }
    for (i=0; i<=n ; i++)
    {
        if(i & bit)
            x^=i;
        else
            y^=i;
    }
    cout<<x<<" and "<<y;
}

int main(int argc, const char * argv[])
{
   
    int i,n,ele;
    cin>>n;
    int* ar=new int[n-2];
    for (i=0; i<n-2; i++)
    {
        cin>>ar[i];
    }
    massuese(ar, n);
    return 0;
}
