#include <iostream>
#include <vector>
#include <queue>
#include <stack>
#include <string>
#include <climits>
#include <stdio.h>
#include <string.h>
#include <cassert>
#include <algorithm>
#include <stack>
#include <map>
#include <list>
#include <set>
#include <math.h>

using namespace std;


void printAllAnagrams(string *s, int n)
{
    string t;
    int i;
    map<string, vector<string> >m;
    for (i=0; i<n ; i++)
    {
        t=s[i];
        sort(t.begin(), t.end());
        m[t].push_back(s[i]);
    }
    map<string, vector<string> >::iterator it;
    for (it=m.begin(); it!=m.end(); it++)
    {
        for (i=0; i<it->second.size(); i++)
        {
            cout<<it->second[i]<<" ";
        }
        cout<<endl;
    }
}

int main(int argc, const char * argv[])
{
    int n,i;
    cin>>n;
    string s[n];
    for (i=0; i<n ; i++)
    {
        cin>>s[i];
    }
    printAllAnagrams(s, n);
    
    return 0;
}


