#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
#include <stack>
#include <deque>
#include <string>
#include <climits>
#include <string.h>
#include <stdio.h>
using namespace std;
#define charToInt(c) ((int)c-(int)'a')

void segregate(int *ar, int n)
{
    int low=0, high=n-1, mid=0;
    while (mid<=high)
    {
        switch (ar[mid])
        {
            case 0: swap(ar[low++], ar[mid++]);
                break;
            case 1: mid++;
                break;
            case 2: swap(ar[mid], ar[high--]);
                break;
        }
    }
}
int main()
{
    int i,n;
    cin>>n;
    int *ar=new int[n];
    for (i=0; i<n ; i++)
    {
        cin>>ar[i];
    }
    cout<<"\nSegregated Array: ";
    segregate(ar, n);
    for (i=0; i<n ; i++)
    {
        cout<<ar[i]<<" ";
    }
    return 0;
}
